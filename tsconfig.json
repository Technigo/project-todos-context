{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true},
    "resolveJsonModule": true,
    "allowJs": true,
    "include": ["src/**/*", "declarations.d.ts"]

}


